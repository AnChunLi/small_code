/**index.wxss**/
.item
{
  width: 90%;
  height: 100px;
  margin: 10px 5%;
  display: flex;
  background-color: #eee;
  border-radius: 5px;
}
.activityimg
{
  width: 100px;
  height: 100px;
  border-radius: 5px;
}
.list-main
{
  margin-left: 8%;
  width: 30%;
}
.item .title
{
  font-size: 20px;
}
.position
{
  margin-top: 40px;
}
.position image
{
  width: 15px;
  height: 15px;
  /* margin-top: 75%; */
}
.position text
{
  font-size: 15px;
  color: #888;
}
.right
{
  display: block;
}
.time
{
  color: #1296db;
  font-size: 15px;
  position: relative;
  top:0px;
  left: 5%;
}
.overtime
{
  color: #888;
  font-size: 15px;
  position: relative;
  top:0px;
  left: 45%;
}
.go
{
margin-top: 40px;
margin-left: 60px;
}
.go image
{
  width: 30px;
  height: 30px;
}


