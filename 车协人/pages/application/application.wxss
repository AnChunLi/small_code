/* pages/application/application.wxss */
.top
{
  background-color: #1296db;
  padding: 10px 0 20px 0;
}
swiper
{
  margin:10px 8%;
}
swiper, block, swiper-item, image
{
  height: 120px;
  border-radius: 5px;
}
.applist
{
  padding: 0 4%;
  display: flex;
}
.item
{
  margin: 10px;
  width: 60px;
}
